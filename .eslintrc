{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2021
    },
    "extends": [
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "func-names": "error",
      "no-console": "error",
      "max-len": [
        "error",
        150
      ],
      "newline-before-return": "error"
    }
  }